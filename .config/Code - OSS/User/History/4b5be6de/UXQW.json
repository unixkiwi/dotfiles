{
  "manifest_version": 2,
  "name": "Custom New Tab",
  "version": "1.0",
  "description": "Set a custom local HTML file as new tab page",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "newtab@example.com",
      "strict_min_version": "42.0"
    }
  }
}